{
	"name": "rust-test-highlight",
	"displayName": "Rust Test Highlight",
	"version": "0.0.1",
	"private": false,
	"description": "Give your rust tests a background color so they visually stand out from the rest of the file",
	"categories": [
		"Other"
	],
	"license": "MIT",
	"type": "commonjs",
	"main": "./dist/extension.js",
	"scripts": {
		"build": "vite build",
		"lint": "eslint src --ext ts",
		"vscode:prepublish": "npm run build"
	},
	"contributes": {
		"colors": [
			{
				"id": "rustTestHighlight.backgroundColor",
				"description": "Background decoration color for tests",
				"defaults": {
					"dark": "#3b3b3b55",
					"light": "#cacaca55",
					"highContrast": "#ebebeb55"
				}
			}
		]
	},
	"activationEvents": [
		"onLanguage:rust"
	],
	"dependencies": {
		"astexplorer-syn": "2.0.15"
	},
	"devDependencies": {
		"@types/node": "20.x",
		"@types/vscode": "^1.86.0",
		"@typescript-eslint/eslint-plugin": "^7.0.1",
		"@typescript-eslint/parser": "^7.0.1",
		"@vscode/vsce": "2.23.0",
		"eslint": "^8.56.0",
		"typescript": "^5.3.3",
		"vite": "5.1.3"
	},
	"engines": {
		"vscode": "^1.86.0"
	},
	"pricing": "Free"
}
